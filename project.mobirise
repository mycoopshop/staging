iv data-for=\"form[data][2][1]\" class=\"mbr-col-lg-4 mbr-col-md-12 mbr-col-sm-12 field\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"form[data][2][1]\" class=\"form-control-label mbr-fonts-style\">Business Name</label>\n<input type=\"hidden\" name=\"form[data][2][0]\" value=\"text\">\n<input type=\"text\" name=\"form[data][2][1]\" placeholder=\"Acme Ltd\" data-form-field=\"text\" mbr-theme-style=\"display-7\" required=\"required\" class=\"field-input\" value=\"\">\n</div>\n<div data-for=\"form[data][3][1]\" class=\"mbr-col-lg-4 mbr-col-md-12 mbr-col-sm-12 field\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"form[data][3][1]\" class=\"form-control-label mbr-fonts-style\">Your Contact Number</label>\n<input type=\"hidden\" name=\"form[data][3][0]\" value=\"Your Contact Number\">\n<input type=\"text\" name=\"form[data][3][1]\" placeholder=\"Phone Number\" data-form-field=\"Your Contact Number\" mbr-theme-style=\"display-7\" required=\"required\" class=\"field-input\" value=\"\">\n</div>\n<div data-for=\"form[data][4][1]\" class=\"mbr-col-lg-4 mbr-col-md-12 mbr-col-sm-12 field\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\"form[data][4][1]\" class=\"form-control-label mbr-fonts-style\">Your Email Address</label>\n<input type=\"hidden\" name=\"form[data][4][0]\" value=\"Your Email Address\">\n<input type=\"text\" name=\"form[data][4][1]\" placeholder=\"name@company.com\" data-form-field=\"Your Email Address\" mbr-theme-style=\"display-7\" required=\"required\" class=\"field-input\" value=\"\">\n</div>\n<div mbr-buttons=\"true\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" class=\"mbr-col-lg-4 mbr-col-md-6 mbr-col-sm-12 field field__btn mbr-flex\">\n<a data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" type=\"submit\" class=\"btn btn-sm btn-form btn-primary\">Submit</a>\n</div>\n</div>\n</form><!--Formbuilder Form-->\n</div>\n            </div>\n              </div>   \